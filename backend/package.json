{
  "name": "ip-rmt64-backend",
  "version": "1.0.0",
  "description": "Backend API with Node.js, Express, and Sequelize for podcast application",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "echo 'Backend is ready for production'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "sequelize": "^6.35.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "openai": "^4.20.1",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "youtube-transcript-api": "^1.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.8"
  },
  "keywords": [
    "nodejs",
    "express",
    "sequelize",
    "postgresql",
    "api",
    "podcast",
    "oauth",
    "openai"
  ],
  "author": "Karon Pangestu",
  "license": "MIT"
}
